{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google", "plugin:prettier/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "require-jsdoc": "off",
    "prettier/prettier": "warn"
  }
}
